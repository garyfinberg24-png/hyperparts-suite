{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hyper-hero-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperHero/HyperHeroWebPart.js",
          "manifest": "./src/webparts/hyperHero/HyperHeroWebPart.manifest.json"
        }
      ]
    },
    "hyper-news-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperNews/HyperNewsWebPart.js",
          "manifest": "./src/webparts/hyperNews/HyperNewsWebPart.manifest.json"
        }
      ]
    },
    "hyper-spotlight-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperSpotlight/HyperSpotlightWebPart.js",
          "manifest": "./src/webparts/hyperSpotlight/HyperSpotlightWebPart.manifest.json"
        }
      ]
    },
    "hyper-profile-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperProfile/HyperProfileWebPart.js",
          "manifest": "./src/webparts/hyperProfile/HyperProfileWebPart.manifest.json"
        }
      ]
    },
    "hyper-tabs-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperTabs/HyperTabsWebPart.js",
          "manifest": "./src/webparts/hyperTabs/HyperTabsWebPart.manifest.json"
        }
      ]
    },
    "hyper-directory-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/hyperDirectory/HyperDirectoryWebPart.js",
          "manifest": "./src/webparts/hyperDirectory/HyperDirectoryWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HyperHeroWebPartStrings": "lib/webparts/hyperHero/loc/{locale}.js",
    "HyperNewsWebPartStrings": "lib/webparts/hyperNews/loc/{locale}.js",
    "HyperSpotlightWebPartStrings": "lib/webparts/hyperSpotlight/loc/{locale}.js",
    "HyperProfileWebPartStrings": "lib/webparts/hyperProfile/loc/{locale}.js",
    "HyperTabsWebPartStrings": "lib/webparts/hyperTabs/loc/{locale}.js",
    "HyperDirectoryWebPartStrings": "lib/webparts/hyperDirectory/loc/{locale}.js"
  }
}
