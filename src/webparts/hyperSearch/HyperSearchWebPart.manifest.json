{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "a1b2c3d4-5e6f-7a8b-9c0d-1e2f3a4b5c6d",
  "alias": "HyperSearchWebPart",
  "componentType": "WebPart",
  "version": "*",
  "manifestVersion": 2,
  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart", "TeamsPersonalApp", "TeamsTab", "SharePointFullPage"],
  "supportsThemeVariants": true,

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70",
    "group": { "default": "HyperParts" },
    "title": { "default": "HyperSearch" },
    "description": { "default": "The most powerful search experience ever built for SharePoint Online. 10 result layouts, 12 templates, instant search, zero-query, smart autocomplete, faceted filters, promoted results, hit highlighting, people cards, quick actions, and federated search across SharePoint, OneDrive, Teams, and Exchange." },
    "officeFabricIconFontName": "SearchBookmark",
    "properties": {
      "title": "Search",
      "placeholderText": "Search everything...",
      "showScopeSelector": true,
      "defaultScope": "everything",
      "defaultSortBy": "relevance",
      "resultsPerPage": 10,
      "enableTypeAhead": true,
      "typeAheadDebounce": 300,
      "enableRefiners": true,
      "refinerFields": "[\"FileType\",\"Author\",\"ContentType\"]",
      "enableSearchHistory": true,
      "promotedResults": "[]",
      "enableAnalytics": false,
      "enableResultPreviews": true,
      "showResultIcon": true,
      "showResultPath": true,
      "selectedTemplate": "modern-clean",
      "resultLayout": "listRich",
      "searchBarStyle": "rounded",
      "accentColor": "#0078d4",
      "borderRadius": 8,
      "activeScopes": "[\"sharepoint\",\"onedrive\"]",
      "showScopeTabs": true,
      "enableDemoMode": true,
      "wizardCompleted": false,
      "showWizardOnInit": true,
      "enableInstantSearch": true,
      "enableSearchVerticals": true,
      "enableZeroQuery": true,
      "enableQuickActions": true,
      "enableHitHighlight": true,
      "enableResultGrouping": false,
      "enableThumbnailPreviews": true,
      "enableSavedSearches": true,
      "enablePeopleCards": true,
      "enableSpellingCorrection": true
    }
  }]
}
